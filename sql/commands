## 1NF Table

-- Part1 : 1NF 

CREATE TABLE Raw_data(
  Student_ID VARCHAR(10) PRIMARY KEY,
  Name VARCHAR(100),
  Email VARCHAR(100),
  Major VARCHAR(50),
  MajorHead VARCHAR(50),
  CourseID VARCHAR(100),
  CourseTitle VARCHAR(100),
  Credits INT,
  Grade VARCHAR(5),
  Building VARCHAR(50),
  Room INT
);

INSERT INTO raw_data 
VALUES
