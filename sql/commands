    ('S103', 'Carol', 'carol@uni.edu', 'Physics', 'Dr.Lee', 'PHYS101' , 'Mechanics' , 4 , 'A' , 'Science', 301 );

-- PART2 : 2NF 

CREATE TABLE Students (
    StudentID VARCHAR(10) PRIMARY KEY,
    Name VARCHAR(50),
    Email VARCHAR(100),
    Major VARCHAR(50)
);


